<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StfPuzzleSimplified.StfPuzzle4x4Activity">

    <RelativeLayout
        android:id="@+id/tablero4"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/buttonn1"
            style="@style/ButtonStyle"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="00"
            android:text="1"/>

        <Button
            android:id="@+id/buttonn2"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn1"
            android:layout_alignParentTop="true"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="01"
            android:text="2"
            />

        <Button
            android:id="@+id/buttonn3"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn2"
            android:layout_alignParentTop="true"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="02"
            android:text="3"/>
        <Button
            android:id="@+id/buttonn4"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn3"
            android:layout_alignParentTop="true"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="03"
            android:text="4"/>
        <Button
            android:id="@+id/buttonn5"
            style="@style/ButtonStyle"
            android:layout_alignParentStart="true"
            android:layout_below="@id/buttonn1"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="10"
            android:text="5"/>

        <Button
            android:id="@+id/buttonn6"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn1"
            android:layout_below="@id/buttonn1"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="11"
            android:text="6"/>

        <Button
            android:id="@+id/buttonn7"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn2"
            android:layout_below="@id/buttonn1"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="12"
            android:text="7"/>
        <Button
            android:id="@+id/buttonn8"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn3"
            android:layout_below="@id/buttonn1"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="13"
            android:text="8"/>
        <Button
            android:id="@+id/buttonn9"
            style="@style/ButtonStyle"
            android:layout_alignParentStart="true"
            android:layout_below="@id/buttonn5"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="20"
            android:text="9"/>

        <Button
            android:id="@+id/buttonn10"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn1"
            android:layout_below="@id/buttonn5"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="21"
            android:text="10"/>

        <Button
            android:id="@+id/buttonn11"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn2"
            android:layout_below="@id/buttonn5"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="22"
            android:text="11"/>
        <Button
            android:id="@+id/buttonn12"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn3"
            android:layout_below="@id/buttonn5"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="23"
            android:text="12"/>
        <Button
            android:id="@+id/buttonn13"
            style="@style/ButtonStyle"
            android:layout_alignParentStart="true"
            android:layout_below="@id/buttonn9"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="30"
            android:text="13"/>

        <Button
            android:id="@+id/buttonn14"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn1"
            android:layout_below="@id/buttonn9"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="31"
            android:text="14"/>

        <Button
            android:id="@+id/buttonn15"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn2"
            android:layout_below="@id/buttonn9"
            app:backgroundTint="@null"
            android:background="@drawable/ic_launcher_background"
            android:tag="32"
            android:text="15"/>
        <Button
            android:id="@+id/buttonn16"
            style="@style/ButtonStyle"
            android:layout_toEndOf="@id/buttonn3"
            android:layout_below="@id/buttonn9"
            app:backgroundTint="@null"
            android:background="@color/transparent"
            android:tag="33"
            android:text="16"/>

    </RelativeLayout>

</FrameLayout>